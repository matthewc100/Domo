usr_alt_city,
usr_alt_st,
usr_alt_zip,
usr_alt_cntry,
usr_alt_cntry_cd,
mgr_usr_nm,
mgr_email_addr,
mgr_full_nm,
sfdc_role_nm,
sfdc_dept_nm,
sfdc_bu_nm,
sfdc_frcst_grp,
