prtnr_func_id,
prtnr_func_pool,
prtnr_func_pool_id,
dell_emp_flag,
dell_emp_id

FROM klue_flag_with_session_data klue 

LEFT JOIN w_resource_d r ON 
	(klue.firstname = r.pers_first_nm 
 AND klue.lastname = r.pers_last_nm 
 OR  klue.dell_employee_number__c = r.pers_id  
 OR  klue.employeenumber = r.pers_id)
 
 GROUP BY
	country, 
    countrycode,
    dell_employee_number__c,
    postalcode,
    state,
	statecode,
	street,
	user_region__c,
	user_sub_region__c,	
	employeenumber,
	employee_number__c,

	klue._batch_id_,
	klue._batch_last_run_,
    klue.id,
    klue.klue_id,
    firstname,
	lastname,
	klue.name,
	klue.klue_id,
	employeenumber,
	klue.email,
	"has klue acct",
	curator,
	"days since last session",
pers_id,
pers_typ,
pers_prfx,
pers_full_nm,
